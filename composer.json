{
    "name": "sparkymelon/mhwilds-sdk",
    "description": "PHP SDK for the Monster Hunter Wilds API",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.3",
        "guzzlehttp/guzzle": "^7.0",
        "vlucas/phpdotenv": "^5.6",
        "brick/json-mapper": "^0.1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "MHWilds\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MHWilds\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
